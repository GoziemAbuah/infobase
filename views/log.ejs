<%- include("partials/header", {title: title, stylesheet: "log"}); %>
<%- include("partials/navbar", {authenticated: true}); %>

<div class="bg">

  <form id="missileSubmit" class="sub" action="/log" method="POST">
    <input id="location" type="text" name="location" value="" required>
    <input id="time" type="text" name="time" value="" required>
  </form>

  <div class="title">Report Missile</div>

  <div id="map"></div>
  <div class="button">
    <button onclick='logMissles()' class="btn btn-success">Submit</button>
  </div>

</div>
<%- include("partials/footer"); %>
